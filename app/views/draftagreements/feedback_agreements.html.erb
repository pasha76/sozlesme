<div class="row">
  <div class="col-lg-12">
    <h1 class="page-header">Yorum alınan sözleşmeler</h1>
  </div>
</div>
<div class="row">
  <div class="col-lg-12">
    <div class="panel panel-default">
      <div class="panel-heading">
        <div class="row">
          <div class="col-md-3">
            Devam eden sözleşme listesi
          </div>

        </div>
      </div>

      <div class="panel-body">
        <table width="100%" class="table table-striped table-bordered table-hover" id="dataTables-agreements">
          <thead>
          <tr>
            <th>Sözleşme tipi</th>
            <th>Üzerinde çalışan</th>
            <th>Hazırlayan</th>
            <th>Hazırlanma tarihi</th>
            <th>Durum</th>
          </tr>
          </thead>
          <tbody>

          <%= render "users/legal/template" %>

          </tbody>
        </table>
      </div>

    </div>
  </div>
</div>

<script>
    $(document).ready(function () {
        $('#dataTables-agreements').DataTable({
            responsive: false
        });
    });
</script>